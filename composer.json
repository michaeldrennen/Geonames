{
  "name": "michaeldrennen/geonames",
  "description": "A Laravel (php) package that interfaces with the geolocation services on geonames.org.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Drennen",
      "email": "michaeldrennen74@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-intl": "*",
    "fabpot/goutte": "^3.2",
    "illuminate/support": "^5.5",
    "illuminate/database": "^5.5",
    "curl/curl": "^1.8",
    "michaeldrennen/remote-file": "^1.0",
    "michaeldrennen/local-file": "^1.0",
    "nesbot/carbon": "^1.22",
    "illuminate/routing": "^5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "orchestra/testbench": "~3.0",
    "satooshi/php-coveralls": "^2.0"
  },
  "autoload": {
    "classmap": [
      "src/Seeders"
    ],
    "psr-4": {
      "MichaelDrennen\\Geonames\\": "src"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "MichaelDrennen\\Geonames\\GeonamesServiceProvider"
      ]
    }
  }
}
